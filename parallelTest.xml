<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="prallel_Test" parallel="tests">
  <test thread-count="1" name="Run on Chrome">
  <parameter name="browser" value="chrome"></parameter>
    <classes>
      <class name="HRM_Framework.HRM_Framework.Login_Test"/>
    </classes>
  </test> <!-- Test -->
  <test thread-count="5" name="Run on Firefox">
  <parameter name="browser" value="firefox"></parameter>
    <classes>
      <class name="HRM_Framework.HRM_Framework.Login_Test"/>
    </classes>
  </test> <!-- Test -->
  <test thread-count="5" name="Run on Edge">
  <parameter name="browser" value="edge"></parameter>
    <classes>
      <class name="HRM_Framework.HRM_Framework.Login_Test"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
